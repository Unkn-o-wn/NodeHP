<h1>{{title}}</h1>

{{#if orders.length}}
{{#each orders}}
<div class="row">
    <div class="cols6offset-s3">
        <div class="card">
            <div class="card-content">
                <sapan class="cart-title">
                    Order <small>{{_id}}</small>
                </sapan>
                <p class="date">{{date}}</p>
                <p><em>
                    {{user.userId.name}}
                </em>
                {{user.userId.email}}
                </p>
                <ol>
                    {{#each courses}}
                        <li>
                           {{course.title}}(x<strong>{{count}}</strong> ) 
                        </li>
                    {{/each}}

                </ol>
                <hr>

                <p>
                    price: <span class="price">{{price}}</span>
                </p>
            </div>
        </div>
    </div>
</div>

{{/each}}
{{else}}
<p>Not have orders</p>
{{/if}}